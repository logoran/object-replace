{
  "name": "object-replace",
  "description": "Object replace like template",
  "version": "1.0.0",
  "homepage": "https://github.com/logoran/object-replace",
  "author": "Leo Long (https://github.com/yujunlong2000)",
  "repository": "logoran/object-replace",
  "bugs": {
    "url": "https://github.com/logoran/object-replace/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "@logoran/clone-deep": "^4.0.0",
    "debug": "^3.1.0",
    "is-plain-object": "^2.0.4"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3"
  },
  "keywords": [
    "replace",
    "array",
    "assign",
    "template",
    "config",
    "replace-array",
    "replace-array-deep",
    "replace-buffer",
    "replace-date",
    "replace-deep",
    "replace-object",
    "replace-object-deep",
    "replace-reg-exp",
    "replace-regex",
    "replace-regexp",
    "deep",
    "extend",
    "object",
    "shallow",
    "symbol"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "is-plain-object",
        "clone-deep"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
